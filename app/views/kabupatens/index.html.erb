<p id="notice"><%= notice %></p>
<div class="container-fluid">
<h1>Data Kabupaten
<br>
<br>
  <%= link_to 'Tambah Kabupaten', new_kabupaten_path, class:'btn btn-primary pull-left' %>
<br>
</h1>

<table class="table">
  <thead>
    <tr>
      <th>Nama kabupaten</th>
      <th>Admin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kabupatens.each do |kabupaten| %>
      <tr>
        <td><%= kabupaten.nama_kabupaten %></td>
        <td><%= kabupaten.admin.nama %></td>
        <td><%= link_to 'Show', kabupaten, class:'btn btn-primary' %></td>
        <td><%= link_to 'Edit', edit_kabupaten_path(kabupaten), class:'btn btn-warning' %></td>
        <td><%= link_to 'Destroy', kabupaten, method: :delete, data: { confirm: 'Anda yakin ingin menghapus?' }, class:'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


