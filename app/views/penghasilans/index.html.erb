<p id="notice"><%= notice %></p>
<div class="container-fluid">
<h1>Penghasilan
<br>
<br>
<%= link_to 'Tambah Penghasilan', new_penghasilan_path, class:'btn btn-primary pull-left' %>
<br>
</h1>

<table class="table">
  <thead>
    <tr>
      <th>Penghasilan</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @penghasilans.each do |penghasilan| %>
      <tr>
        <td><%= penghasilan.penghasilan %></td>
        <td><%= link_to 'Show', penghasilan, class:'btn btn-primary' %></td>
        <td><%= link_to 'Edit', edit_penghasilan_path(penghasilan), class:'btn btn-warning' %></td>
        <td><%= link_to 'Destroy', penghasilan, method: :delete, data: { confirm: 'Anda yakin ingin menghapus?' }, class:'btn btn-danger' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>


